include("plan_funcs.php");

$days = array();

for($i = 1; $i <= 20; $i++) {
  $b = $i + 21;
  $days["August $i"] = "Jeremiah $b";
}

for($i = 21; $i <=29; $i++) {
  $b = $i - 20;
  $days["August $i"] = "Amos $b";
}

$days["August 30"] = "Obadiah 1";
$days["August 31"] = "Philemon 1";

$YEAR = "2013";

echo_list($days, $YEAR);

